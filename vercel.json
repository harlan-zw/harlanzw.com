{
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/:path",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=79344"
        }
      ]
    },
    {
      "source": "/blog/:path",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=79344"
        }
      ]
    },
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=79344"
        }
      ]
    }
  ]
}
